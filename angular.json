{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "xotb-demo": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/xotb-demo",
      "sourceRoot": "projects/xotb-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/xotb-demo",
            "index": "projects/xotb-demo/src/index.html",
            "main": "projects/xotb-demo/src/main.ts",
            "polyfills": "projects/xotb-demo/src/polyfills.ts",
            "tsConfig": "projects/xotb-demo/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/xotb-demo/src/favicon.ico",
              "projects/xotb-demo/src/assets"
            ],
            "styles": ["projects/xotb-demo/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/xotb-demo/src/environments/environment.ts",
                  "with": "projects/xotb-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "xotb-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "xotb-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "xotb-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/xotb-demo/src/test.ts",
            "polyfills": "projects/xotb-demo/src/polyfills.ts",
            "tsConfig": "projects/xotb-demo/tsconfig.spec.json",
            "karmaConfig": "projects/xotb-demo/karma.conf.js",
            "assets": [
              "projects/xotb-demo/src/favicon.ico",
              "projects/xotb-demo/src/assets"
            ],
            "styles": ["projects/xotb-demo/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/xotb-demo/tsconfig.app.json",
              "projects/xotb-demo/tsconfig.spec.json",
              "projects/xotb-demo/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/xotb-demo/e2e/protractor.conf.js",
            "devServerTarget": "xotb-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "xotb-demo:serve:production"
            }
          }
        }
      }
    },
