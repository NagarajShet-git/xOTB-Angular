    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "9.2.0",
    "lodash": "^4.17.15",
    "marked": "^0.7.0",
    "ncp": "^2.0.0",
    "ng-packagr": "^5.1.0",
    "prettier": "1.18.2",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.0",
    "sass": "^1.22.12",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
